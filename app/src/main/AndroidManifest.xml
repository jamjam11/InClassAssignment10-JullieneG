<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="julliene.inclassassignment10_jullieneg">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ObjectActivity"
            android:label="BlogPost Object"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ListActivity"
            android:label="Read BlogPosts"
            android:parentActivityName=".ListActivity"/>
        <activity android:name=".CameraActivity"
            android:label="Camera"
            android:parentActivityName=".CameraActivity"/>
        <activity android:name=".StorageActivity"
            android:label="Load Images"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>